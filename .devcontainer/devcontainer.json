{
  "name": "Robotnik",
  "dockerComposeFile": "docker-compose.yml",
  "service": "robotnik",
  "workspaceFolder": "/home/ros/robot_ws",
  "remoteUser": "ros",
  "postCreateCommand": "/home/ros/postcreate.sh",

  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-python.python",
        "GitHub.copilot",
        "PKief.material-icon-theme",
        "redhat.vscode-xml",
        "redhat.vscode-yaml"
      ]
    }
  }
}

